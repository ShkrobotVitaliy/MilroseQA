<apex:page controller="ProjectManagerHomePageController" sidebar="false" showHeader="false" >
    <style type="text/css">
        .Mylinks{
            font-size: 16px;
        }
    </style>
    <apex:pageBlock >
       <apex:tabPanel switchType="client" selectedTab="report1" id="theTabPanel" >
            <apex:tab switchType="client" label="Trailing Revenue" name="report1" id="tabOne" style="overflow:hidden;width:100%;height:900px">
                <apex:outputLink value="/01ZA0000000ddSb?isdtp=nv" target="_blank" styleClass="Mylinks" id="theLink1">Detail View</apex:outputLink>
                    <apex:iframe src="/01ZA0000000ddSb?isdtp=nv" height="860px" width="100%" scrolling="true"/>
            </apex:tab>
            <apex:tab switchType="client" label="Revenue" name="report2" id="tabTwo" style="overflow:auto;width:100%;height:900px">
                <apex:outputLink value="/01ZA0000000dnT6?isdtp=nv" target="_blank" styleClass="Mylinks" id="theLink2">Detail View</apex:outputLink>
                    <apex:iframe src="/01ZA0000000dnT6?isdtp=nv" height="600px" width="100%" scrolling="true" />
            </apex:tab>
            <apex:tab switchType="client" label="Proposals" name="report3" id="tabThree" style="overflow:hidden;width:100%;height:900px">
                 <apex:outputLink value="/01ZA0000000dnT7?isdtp=nv" target="_blank" styleClass="Mylinks" id="theLink3">Detail View</apex:outputLink>
                    <apex:iframe src="/01ZA0000000dnT7?isdtp=nv" height="860px" width="100%" scrolling="true"/>
            </apex:tab>
            <apex:tab switchType="client" label="Top 25 Accounts" name="report4" id="tabFour" style="overflow:hidden;width:100%;height:900px">
                 <apex:outputLink value="/01ZA0000000dp3U?isdtp=nv" target="_blank" styleClass="Mylinks" id="theLink4">Detail View</apex:outputLink>
                    <apex:iframe src="/01ZA0000000dp3U?isdtp=nv" height="860px" width="100%" scrolling="true"/>
            </apex:tab>
        </apex:tabPanel>
        <div style="{!$Label.CopyRightInfoStyle}">{!$Label.CopyrightInfo}</div>
    </apex:pageBlock>
</apex:page>